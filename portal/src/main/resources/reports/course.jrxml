	<queryString>
		<![CDATA[SELECT C.COURSE_NAME,
		C.COURSE_SORTATION,
		C.COURSE_CODE,
		D.DEPART_NAME,
		P.PROFESSOR_NAME,
		C.COURSE_CREDIT,
		P.PROFESSOR_EMAIL,
		C.COURSE_SUMMARY
